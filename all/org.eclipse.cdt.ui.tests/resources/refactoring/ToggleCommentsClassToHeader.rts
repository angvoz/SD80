//!ClassToHeaderTrailingMultipleCommentsInTryBlock
//#org.eclipse.cdt.ui.tests.refactoring.togglefunction.ToggleRefactoringTest
//@.config
filename=A.h
//@A.h
class A {
    int /*$*/member/*$$*/() try
    {
        // aans
    } /* one */ catch (int i) {
        // zwaa
    } /* two */ catch (int j) {
        // draa
    } /* three */
};
//=
class A {
    int member();
};

inline int A::member()
try
{
    // aans
} /* one */
catch(int i){
    // zwaa
}
 /* two */catch(int j){
    // draa
}
 /* three */

