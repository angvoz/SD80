      </view>
      <category
            name="%CBrowsing.viewCategoryName"
            id="org.eclipse.cdt.ui.c.browsing">
      </category>
      <view
            name="%CBrowsing.projectsViewName"
            icon="icons/full/eview16/cprojects.gif"
