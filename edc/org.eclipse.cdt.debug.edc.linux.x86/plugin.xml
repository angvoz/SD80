<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.cdt.debug.edc.tcfAgentLauncher">
      <descriptor
            class="org.eclipse.cdt.debug.edc.linux.x86.GdbserverAgentDescriptor">
      </descriptor>
   </extension>

  <extension
        point="org.eclipse.debug.core.launchDelegates">
     <launchDelegate
           delegate="org.eclipse.cdt.debug.edc.linux.x86.launch.LinuxX86LaunchDelegate"
           delegateDescription="Start new process under control of EDC debugger"
           id="org.eclipse.cdt.debug.edc.linux.x86.launchDelegate"
           modes="run,debug"
           name="EDC Create Process (Linux)"
           sourceLocatorId="org.eclipse.cdt.debug.ui.DefaultSourceLocator"
           sourcePathComputerId="org.eclipse.cdt.debug.core.sourcePathComputer"
           type="org.eclipse.cdt.launch.applicationLaunchType">
     </launchDelegate>
     <launchDelegate
           delegate="org.eclipse.cdt.debug.edc.linux.x86.launch.LinuxX86LaunchDelegate"
           delegateDescription="Attach EDC debugger to a running program."
           id="org.eclipse.cdt.debug.edc.linux.x86.attachLaunchDelegate"
           modes="debug"
           name="EDC Attach to process (Linux)"
           sourceLocatorId="org.eclipse.cdt.debug.core.sourceLocator"
           sourcePathComputerId="org.eclipse.cdt.debug.core.sourcePathComputer"
           type="org.eclipse.cdt.launch.attachLaunchType">
     </launchDelegate>
  </extension>
  <extension
        point="org.eclipse.debug.ui.launchConfigurationTabs">
     <tab
           class="org.eclipse.cdt.launch.ui.CMainTab"
           group="org.eclipse.cdt.launch.applicationLaunchTabGroup"
           id="org.eclipse.cdt.debug.edc.ui.mainTab"
           name="Main">
        <associatedDelegate
              delegate="org.eclipse.cdt.debug.edc.linux.x86.launchDelegate">
        </associatedDelegate>
     </tab>
     <tab
           class="org.eclipse.cdt.launch.ui.CArgumentsTab"
           group="org.eclipse.cdt.launch.applicationLaunchTabGroup"
           id="org.eclipse.cdt.debug.edc.ui.argumentsTab"
           name="Arguments">
        <associatedDelegate
              delegate="org.eclipse.cdt.debug.edc.linux.x86.launchDelegate">
        </associatedDelegate>
        <placement
              after="org.eclipse.cdt.cdi.launch.mainTab">
        </placement>
     </tab>
     <tab
           class="org.eclipse.debug.ui.EnvironmentTab"
           group="org.eclipse.cdt.launch.applicationLaunchTabGroup"
           id="org.eclipse.cdt.debug.edc.ui.environmentTab"
           name="Environment">
        <associatedDelegate
              delegate="org.eclipse.cdt.debug.edc.linux.x86.launchDelegate">
        </associatedDelegate>
        <placement
              after="org.eclipse.cdt.cdi.launch.argumentsTab">
        </placement>
     </tab>
     <tab
           class="org.eclipse.cdt.debug.edc.internal.ui.launch.RegularDebuggerTab"
           group="org.eclipse.cdt.launch.applicationLaunchTabGroup"
           id="org.eclipse.cdt.debug.edc.ui.debuggerTab"
           name="Debugger (regular)">
        <associatedDelegate
              delegate="org.eclipse.cdt.debug.edc.linux.x86.launchDelegate">
        </associatedDelegate>
        <placement
              after="org.eclipse.debug.ui.environmentTab">
        </placement>
     </tab>
     <tab
           class="org.eclipse.debug.ui.sourcelookup.SourceLookupTab"
           group="org.eclipse.cdt.launch.applicationLaunchTabGroup"
           id="org.eclipse.cdt.debug.edc.ui.sourceTab"
           name="Source">
        <associatedDelegate
              delegate="org.eclipse.cdt.debug.edc.linux.x86.launchDelegate">
        </associatedDelegate>
        <placement
              after="org.eclipse.cdt.debug.edc.ui.launch.regularDebuggerTab">
        </placement>
     </tab>
     <tab
           class="org.eclipse.debug.ui.CommonTab"
           group="org.eclipse.cdt.launch.applicationLaunchTabGroup"
           id="org.eclipse.cdt.debug.edc.ui.commonTab"
           name="Common">
        <associatedDelegate
              delegate="org.eclipse.cdt.debug.edc.linux.x86.launchDelegate">
        </associatedDelegate>
        <placement
              after="org.eclipse.debug.ui.sourceLookupTab">
        </placement>
     </tab>
  </extension>

 <extension
       point="org.eclipse.debug.ui.launchConfigurationTabs">
    <tab
          class="org.eclipse.cdt.debug.edc.internal.ui.launch.AttachDebuggerTab"
          group="org.eclipse.cdt.launch.attachLaunchTabGroup"
          id="org.eclipse.cdt.debug.edc.ui.attachDebuggerTab"
          name="Debugger (attach)">
       <associatedDelegate
             delegate="org.eclipse.cdt.debug.edc.linux.x86.attachLaunchDelegate">
       </associatedDelegate>
    </tab>
    <tab
          class="org.eclipse.debug.ui.sourcelookup.SourceLookupTab"
          group="org.eclipse.cdt.launch.attachLaunchTabGroup"
          id="org.eclipse.cdt.debug.edc.ui.attachSourceTab"
          name="Source">
       <associatedDelegate
             delegate="org.eclipse.cdt.debug.edc.linux.x86.attachLaunchDelegate">
       </associatedDelegate>
       <placement
             after="org.eclipse.cdt.debug.edc.ui.launch.attachDebuggerTab">
       </placement>
    </tab>
    <tab
          class="org.eclipse.debug.ui.CommonTab"
          group="org.eclipse.cdt.launch.attachLaunchTabGroup"
          id="org.eclipse.cdt.debug.edc.ui.attachCommonTab"
          name="Common">
       <associatedDelegate
             delegate="org.eclipse.cdt.debug.edc.linux.x86.attachLaunchDelegate">
       </associatedDelegate>
       <placement
             after="org.eclipse.debug.ui.sourceLookupTab">
       </placement>
    </tab>
 </extension>	

</plugin>
