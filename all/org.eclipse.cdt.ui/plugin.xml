            category="org.eclipse.cdt.ui.newCCWizards"
            class="org.eclipse.cdt.ui.wizards.StdCCWizard"
            project="true"
            finalPerspective="org.eclipse.cdt.ui.CPerspective"
            id="org.eclipse.cdt.ui.wizards.StdCCWizard">
