            icon="icons/full/obj16/build_menu.gif"
            category="org.eclipse.cdt.ui.views"
            class="org.eclipse.cdt.internal.ui.makeview.MakeView"
            id="org.eclipse.cdt.ui.MakeView">
      </view>
   </extension>
<!-- The wizards -->
<!-- For C Wizards -->
