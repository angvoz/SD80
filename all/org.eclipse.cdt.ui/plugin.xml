            class="org.eclipse.cdt.ui.wizards.conversion.ConvertToStdMakeConversionWizard"
            project="true"
            finalPerspective="org.eclipse.cdt.ui.CPerspective"
            id="org.eclipse.cdt.ui.wizards.ConvertToStdMakeConversionWizard">
