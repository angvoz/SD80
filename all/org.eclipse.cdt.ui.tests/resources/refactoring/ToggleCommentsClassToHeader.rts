//!ClassToHeaderCatchComment
//#org.eclipse.cdt.ui.tests.refactoring.togglefunction.ToggleRefactoringTest
//@.config
filename=A.h
//@A.h
class A {
    void /*$*/member/*$$*/() try
    {
        return;
    } catch (int i) {
        // catch comment
    }
};
//=
class A {
    void member();
};

inline void A::member()
try
{
    return;
}
catch(int i){
    // catch comment
}


