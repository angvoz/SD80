//!ExtractLocalVariableRefactoringTest int
//#org.eclipse.cdt.ui.tests.refactoring.extractlocalvariable.ExtractLocalVariableRefactoringTest
//@A.h
#ifndef A_H_
#define A_H_

class A
{
public:
  A();
  virtual ~A();
  int foo();
};

#endif /*A_H_*/

//=
#ifndef A_H_
#define A_H_

class A
{
public:
  A();
  virtual ~A();
  int foo();
};

#endif /*A_H_*/

//@A.cpp
#include "A.h"

A::A()
{
}

A::~A()
{
}

int A::foo()
{
  return /*$*/42/*$$*/;
}

//=
#include "A.h"

A::A()
{
}

A::~A()
{
}

int A::foo()
{
    int i = 42;
    return i;
}

