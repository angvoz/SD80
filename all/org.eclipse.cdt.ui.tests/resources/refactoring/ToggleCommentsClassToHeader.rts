//!ClassToHeaderBodyComment
//#org.eclipse.cdt.ui.tests.refactoring.togglefunction.ToggleRefactoringTest
//@.config
filename=A.h
//@A.h
class A {
    void /*$*/member/*$$*/()
    {
        // return comment
        return;
    }
};
//=
class A {
    void member();
};

inline void A::member()
{
    // return comment
    return;
}


